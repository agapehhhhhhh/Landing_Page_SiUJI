# ===========================================
# SIUJI Frontend - Environment Variables
# ===========================================

# Application Configuration
VITE_APP_NAME=SIUJI
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Platform Ujian Digital untuk Pembelajaran Modern

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_CMS_URL=http://localhost:3000
VITE_MEDIA_URL=http://localhost:3000/media

# Environment
VITE_NODE_ENV=development

# Features Toggle
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=false

# External Services
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here

# UI Configuration
VITE_DEFAULT_LANGUAGE=id
VITE_THEME_MODE=light
VITE_ENABLE_DARK_MODE=true

# Contact Information
VITE_CONTACT_EMAIL=info@siuji.id
VITE_CONTACT_PHONE=+62 22 7796 8800
VITE_COMPANY_ADDRESS=Jakarta, Indonesia

# Social Media
VITE_FACEBOOK_URL=https://facebook.com/siuji
VITE_TWITTER_URL=https://twitter.com/siuji
VITE_INSTAGRAM_URL=https://instagram.com/siuji
VITE_LINKEDIN_URL=https://linkedin.com/company/siuji